@namespace exprGrammar
@classname Grammar


start <Node>
	= logical

logical <Node>
	= primary

primary <Node>
  = integer
  / "(" val:logical ")" {val}
  / string

integer <ValueNode<int>>
	= num:([0-9]*) { new ValueNode<int>(int.Parse(Flatten(num)))}

string <ValueNode<string>>
	= "\"" text:([^"]*) { new ValueNode<string>(Flatten(text))}